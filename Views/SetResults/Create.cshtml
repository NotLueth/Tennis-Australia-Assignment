@model TennisAustraliaAssignment.Models.SetResult

@{
    ViewData["Title"] = "Create";
}
<!-- Header of Page -->
<h1>Create</h1>
<!-- Header of area -->
<h4>SetResult</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <!-- Set Field -->
                <label asp-for="set" class="control-label"></label>
                <input asp-for="set" class="form-control" />
                <span asp-validation-for="set" class="text-danger"></span>
            </div>
            <!-- First Registration Score Field -->
            <div class="form-group">
                <label asp-for="FirstRegIDScore" class="control-label"></label>
                <input asp-for="FirstRegIDScore" class="form-control" />
                <span asp-validation-for="FirstRegIDScore" class="text-danger"></span>
            </div>
            <!-- Second Registation Score Field -->
            <div class="form-group">
                <label asp-for="SecondRegIDScore" class="control-label"></label>
                <input asp-for="SecondRegIDScore" class="form-control" />
                <span asp-validation-for="SecondRegIDScore" class="text-danger"></span>
            </div>
            <!-- Match ID Field -->
            <div class="form-group">
                <label asp-for="MatchID" class="control-label"></label>
                <select asp-for="MatchID" class="form-control" asp-items="ViewBag.MatchID"></select>
            </div>
            <!-- Save button -->
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
